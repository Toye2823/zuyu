.searchTem 
{
  width:100%;
  height:100%;
  background: #fff;
  overflow: hidden;
}
.searchTem .search
{
  display: flex;
  width:90%;
  margin: 0 auto;
  background:#fff;
  padding:10px;
}
.searchTem .searchInput
{
  border:1px solid #ccc;
  border-right:none;
  border-radius:5px 0 0 5px;
  width:90%;
  height:30px;
  padding-left:5px;
  font-size:12px;

}

.searchTem icon 
{
  width:10%;
  height:30px;
  border:1px solid #ccc;
  border-left:none;
  border-radius:0 5px 5px 0;
  font-size:20px;
  color:#ccc;

}
/*banner 列表*/
.bannerTem swiper image
{
    min-height:250px;
}
/*列表*/
.itemListTem 
{
   display: block;
   overflow:hidden;
   background: #fff;
   margin:10px 0 0 0;


}
.itemListTem .list
{
  height:55px;
  float:left;
  margin:20px 0;
}
.itemListTem .list .item-img
{
  height:50px;
  width:50px;
  overflow:hidden;
  margin:0 auto;
  text-align:center;

}

.itemListTem .list .item-img image
{
  height:50ppx;
  width:50px;
  margin:0 auto;
}
.itemListTem .list .item-name
{
  height:100%;
  width:100%;
  text-align:center;
  color:#333;
  font-size:14px;
  font-weight:700;
  margin:5px 0 0 0;

}

/*可移动联系电话*/
.phoneTem{
  position:fixed;
  z-index: 9999;
  height:40px; 
  line-height:40px;
  width:40px; 
  right:0;
  bottom:100px;
  border-radius:100%;
  text-align: center;
  padding: 0;
  background: none;

}
.phoneTem icon
{
  font-size:40px;
  color:#555;
  background: #fff;
  padding:0;
  margin: 0;
}

.phoneTem.button,
button[class="phoneTem"]::after {
  background: none;
  border:none;
}
